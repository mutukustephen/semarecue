[
  {
    "id": "cpr_adult",
    "title_en": "CPR (Adult)",
    "title_sw": "CPR (Mtu Mzima)",
    "icon_asset": "ic_cpr",
    "category": "Critical",
    "keywords": "heart, breathing, chest, cardiac arrest, moyo, pumzi, kifua",
    "steps": [
      {
        "text_en": "Check for responsiveness. Shake shoulders and shout 'Are you okay?'",
        "text_sw": "Angalia kama anaitikia. Tikisa mabega na uliza kwa sauti 'Uko sawa?'",
        "audio_asset_en": "cpr_step1_en.mp3",
        "audio_asset_sw": "cpr_step1_sw.mp3",
        "graphic_asset": "img_cpr_check",
        "timer_seconds": null
      },
      {
        "text_en": "Call 999 or 112 immediately if no response.",
        "text_sw": "Piga 999 au 112 mara moja kama haitiki.",
        "audio_asset_en": "cpr_step2_en.mp3",
        "audio_asset_sw": "cpr_step2_sw.mp3",
        "graphic_asset": "img_call_help",
        "timer_seconds": null
      },
      {
        "text_en": "Place heel of hand on center of chest. Interlock fingers.",
        "text_sw": "Weka kisigino cha mkono katikati ya kifua. Unganisha vidole.",
        "audio_asset_en": "cpr_step3_en.mp3",
        "audio_asset_sw": "cpr_step3_sw.mp3",
        "graphic_asset": "img_cpr_hand_pos",
        "timer_seconds": null
      },
      {
        "text_en": "Push hard and fast. 100-120 compressions per minute. Compress at least 2 inches deep.",
        "text_sw": "Bonyeza kwa nguvu na haraka. Migandamizo 100-120 kwa dakika. Bonyeza kirefu cha inchi 2.",
        "audio_asset_en": "cpr_step4_en.mp3",
        "audio_asset_sw": "cpr_step4_sw.mp3",
        "graphic_asset": "img_cpr_compress",
        "timer_seconds": 120
      },
      {
        "text_en": "Continue until help arrives or person starts breathing.",
        "text_sw": "Endelea hadi msaada ufike au mtu aanze kupumua.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      }
    ]
  },
  {
    "id": "cpr_child",
    "title_en": "CPR (Child 1-8 years)",
    "title_sw": "CPR (Mtoto 1-8 miaka)",
    "icon_asset": "ic_cpr_child",
    "category": "Critical",
    "keywords": "child, kid, breathing, heart, mtoto, pumzi, moyo",
    "steps": [
      {
        "text_en": "Check for responsiveness. Tap shoulder gently and call their name.",
        "text_sw": "Angalia kama anaitikia. Gusa bega pole na mwite kwa jina.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_cpr_child_check",
        "timer_seconds": null
      },
      {
        "text_en": "Call 999 or 112 immediately if no response.",
        "text_sw": "Piga 999 au 112 mara moja kama haitiki.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_call_help",
        "timer_seconds": null
      },
      {
        "text_en": "Use one or two hands on center of chest. Use less force than adult CPR.",
        "text_sw": "Tumia mkono mmoja au miwili katikati ya kifua. Tumia nguvu kidogo kuliko CPR ya wazima.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_cpr_child_hands",
        "timer_seconds": null
      },
      {
        "text_en": "Compress about 2 inches deep at 100-120 per minute.",
        "text_sw": "Bonyeza kirefu cha inchi 2 kwa kasi ya 100-120 kwa dakika.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_cpr_child_compress",
        "timer_seconds": 120
      }
    ]
  },
  {
    "id": "cpr_infant",
    "title_en": "CPR (Infant under 1 year)",
    "title_sw": "CPR (Mtoto chini ya mwaka 1)",
    "icon_asset": "ic_cpr_infant",
    "category": "Critical",
    "keywords": "baby, infant, newborn, mtoto mchanga, pumzi",
    "steps": [
      {
        "text_en": "Check for responsiveness. Tap foot gently.",
        "text_sw": "Angalia kama anaitikia. Gusa mguu pole.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_infant_check",
        "timer_seconds": null
      },
      {
        "text_en": "Call 999 or 112 immediately if no response.",
        "text_sw": "Piga 999 au 112 mara moja kama haitiki.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_call_help",
        "timer_seconds": null
      },
      {
        "text_en": "Use two fingers on center of chest, just below nipple line.",
        "text_sw": "Tumia vidole viwili katikati ya kifua, chini kidogo ya mistari ya chuchu.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_infant_fingers",
        "timer_seconds": null
      },
      {
        "text_en": "Compress about 1.5 inches deep at 100-120 per minute.",
        "text_sw": "Bonyeza kirefu cha inchi 1.5 kwa kasi ya 100-120 kwa dakika.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_infant_compress",
        "timer_seconds": 120
      }
    ]
  },
  {
    "id": "choking_adult",
    "title_en": "Choking (Adult)",
    "title_sw": "Kunasa (Mtu Mzima)",
    "icon_asset": "ic_choking",
    "category": "Critical",
    "keywords": "choking, airway, coughing, kunasa, kukohoa, hewa",
    "steps": [
      {
        "text_en": "Ask 'Are you choking?' If they can't speak or cough, act immediately.",
        "text_sw": "Uliza 'Unanasa?' Kama hawawezi kusema au kukohoa, fanya haraka.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_choking_check",
        "timer_seconds": null
      },
      {
        "text_en": "Stand behind them. Make a fist above their navel.",
        "text_sw": "Simama nyuma yao. Fanya ngumi juu ya kitovu.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_heimlich_position",
        "timer_seconds": null
      },
      {
        "text_en": "Give 5 quick upward thrusts (Heimlich maneuver).",
        "text_sw": "Toa misukumo 5 ya haraka kuelekea juu (Heimlich).",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_heimlich_thrust",
        "timer_seconds": null
      },
      {
        "text_en": "Repeat until object comes out or person becomes unconscious.",
        "text_sw": "Rudia hadi kitu kitoke au mtu apoteze fahamu.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      },
      {
        "text_en": "If unconscious, start CPR and call 999/112.",
        "text_sw": "Kama amepoteza fahamu, anza CPR na piga 999/112.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      }
    ]
  },
  {
    "id": "choking_child",
    "title_en": "Choking (Child/Infant)",
    "title_sw": "Kunasa (Mtoto/Mchanga)",
    "icon_asset": "ic_choking_child",
    "category": "Critical",
    "keywords": "child choking, baby, infant, mtoto kunasa",
    "steps": [
      {
        "text_en": "For infant: Give 5 back blows between shoulder blades with heel of hand.",
        "text_sw": "Kwa mtoto mchanga: Toa mapigo 5 ya mgongo kati ya mabega kwa kisigino.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_infant_back_blow",
        "timer_seconds": null
      },
      {
        "text_en": "Turn infant face up. Give 5 chest thrusts with two fingers.",
        "text_sw": "Geuza mtoto uso juu. Toa misukumo 5 ya kifua kwa vidole viwili.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_infant_chest_thrust",
        "timer_seconds": null
      },
      {
        "text_en": "For child: Use modified Heimlich with less force.",
        "text_sw": "Kwa mtoto: Tumia Heimlich iliyobadilishwa kwa nguvu kidogo.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_child_heimlich",
        "timer_seconds": null
      },
      {
        "text_en": "Call 999/112 immediately if object doesn't come out.",
        "text_sw": "Piga 999/112 mara moja kama kitu hakitoki.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      }
    ]
  },
  {
    "id": "heart_attack",
    "title_en": "Heart Attack",
    "title_sw": "Mshtuko wa Moyo",
    "icon_asset": "ic_heart_attack",
    "category": "Critical",
    "keywords": "chest pain, heart, cardiac, maumivu ya kifua, moyo",
    "steps": [
      {
        "text_en": "Call 999 or 112 immediately. Don't wait!",
        "text_sw": "Piga 999 au 112 mara moja. Usisubiri!",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_call_emergency",
        "timer_seconds": null
      },
      {
        "text_en": "Have person sit down and rest. Loosen tight clothing.",
        "text_sw": "Mwambie mtu aketi na apumzike. Legeza nguo zinazobana.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_sit_rest",
        "timer_seconds": null
      },
      {
        "text_en": "Give aspirin if available and person is not allergic (chew, don't swallow).",
        "text_sw": "Toa aspirini kama ipo na mtu hana mzio (tafuna, usimeze).",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_aspirin",
        "timer_seconds": null
      },
      {
        "text_en": "Monitor breathing. Be ready to perform CPR if needed.",
        "text_sw": "Angalia upumuaji. Kuwa tayari kufanya CPR ikihitajika.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      }
    ]
  },
  {
    "id": "stroke",
    "title_en": "Stroke",
    "title_sw": "Kiharusi/Stroke",
    "icon_asset": "ic_stroke",
    "category": "Critical",
    "keywords": "stroke, paralysis, face drooping, kiharusi, kupooza",
    "steps": [
      {
        "text_en": "Remember FAST: Face drooping, Arm weakness, Speech difficulty, Time to call 999/112.",
        "text_sw": "Kumbuka FAST: Uso kupooza, Mkono kulegea, Usemi ugumu, Wakati wa kupiga 999/112.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_fast_stroke",
        "timer_seconds": null
      },
      {
        "text_en": "Call 999/112 immediately. Note the time symptoms started.",
        "text_sw": "Piga 999/112 mara moja. Kumbuka wakati dalili zilianza.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_call_emergency",
        "timer_seconds": null
      },
      {
        "text_en": "Keep person calm and lying down with head slightly elevated.",
        "text_sw": "Mwambie mtu atulie na alale na kichwa kikiwa juu kidogo.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_elevated_head",
        "timer_seconds": null
      },
      {
        "text_en": "Do NOT give food, drink, or medication.",
        "text_sw": "USITOE chakula, kinywaji, au dawa.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      }
    ]
  },
  {
    "id": "severe_bleeding",
    "title_en": "Severe Bleeding",
    "title_sw": "Kutokwa na Damu Sana",
    "icon_asset": "ic_bleeding",
    "category": "Critical",
    "keywords": "blood, cut, wound, hemorrhage, damu, kidonda, jeraha",
    "steps": [
      {
        "text_en": "Call 999/112 if bleeding is severe or won't stop.",
        "text_sw": "Piga 999/112 kama damu inatoka sana au haachi.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_call_emergency",
        "timer_seconds": null
      },
      {
        "text_en": "Apply direct pressure to the wound with a clean cloth or bandage.",
        "text_sw": "Bonyeza kidonda moja kwa moja na kitambaa safi au bendeji.",
        "audio_asset_en": "bleed_step1_en.mp3",
        "audio_asset_sw": "bleed_step1_sw.mp3",
        "graphic_asset": "img_bleed_pressure",
        "timer_seconds": null
      },
      {
        "text_en": "Keep pressure for at least 10 minutes. Do not remove the cloth.",
        "text_sw": "Endelea kubonyeza kwa dakika 10. Usitoe kitambaa.",
        "audio_asset_en": "bleed_step2_en.mp3",
        "audio_asset_sw": "bleed_step2_sw.mp3",
        "graphic_asset": "img_bleed_hold",
        "timer_seconds": 600
      },
      {
        "text_en": "If blood soaks through, add more cloth on top. Don't remove the first one.",
        "text_sw": "Kama damu inapenya, ongeza kitambaa juu. Usitoe cha kwanza.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": null,
        "timer_seconds": null
      },
      {
        "text_en": "Elevate the injured area above heart level if possible.",
        "text_sw": "Inua sehemu iliyoumia juu ya kiwango cha moyo ikiwezekana.",
        "audio_asset_en": null,
        "audio_asset_sw": null,
        "graphic_asset": "img_elevate_limb",
        "timer_seconds": null
      }
    ]
  },
  {
    "id": "anaphylaxis",
    "title_en": "Severe Allergic Reaction (Anaphylaxis)",
    "title_sw": "Mzio Mkubwa (Anaphylaxis)",
    "icon_asset": "ic_allergy",
    "category": "Critical",
    "keywords": "allergy, swelling, breathing, anaphylaxis, mzio, uvimbe, pumzi",
    "steps": [
      {
